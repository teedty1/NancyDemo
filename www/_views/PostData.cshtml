@{
    Layout = "Layout.cshtml";
}
@model www.PostData
@section HeaderContent {
    <h1 class="display-4 text-white mt-5 mb-2">Nancy Post Data</h1>
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>FirstName: @Model.FirstName</h1>
            <h1>LastName: @Model.LastName</h1>
            <h1>GUID: @Model.Guid</h1>
        </div>
    </div>
</div>
